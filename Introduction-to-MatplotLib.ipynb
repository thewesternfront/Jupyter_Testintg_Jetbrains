    "   \"source\": [\n",
    "    \"# 1 Import MaoplotLib\\n\",\n",
    "    \"import matplotlib.pyplot as plt\\n\",\n",
    "    \"# 2 prepare data x=[], y=[]\\n\",\n",
    "    \"# 3 Set up the plot\\n\",\n",
    "    \"fig, ax = plt.subplots(figsize = (10, 5))  # width, height, this is the aspect ratio, 5,10 is portrait, 10,5 is landscape\\n\",\n",
    "    \"# 4 plot data\\n\",\n",
    "    \"ax.plot(x, y)\\n\",\n",
    "    \"# 5 Customize Plot\\n\",\n",
    "    \"ax.set(title = \\\"simple plot\\\", xlabel = \\\"x axis\\\", ylabel = \\\"y axis\\\")\\n\",\n",
    "    \"# 6 Save & Show\\n\",\n",
    "    \"fig.savefig(\\\"sampleplot.png\\\")\\n\"\n",
